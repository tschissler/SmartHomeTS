{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "SmartHome.Web": "Information",
      "SmartHome.Web.Services": "Information"
    }
  },
  "AllowedHosts": "*",
  "DetailedErrors": true,
